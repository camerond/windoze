!!!
%html
  %head
    %title Windoze Test Suite
    %script(type="text/javascript" src="/javascripts/jquery.min.js")
    %script(type="text/javascript" src="/lib/qunit.js")
    %script(type="text/javascript" src="/javascripts/jquery.windoze.js")
    %script(type="text/javascript" src="/javascripts/suite.js")
    %link(rel="stylesheet" href="/lib/qunit.css" type="text/css")
  %body
    %h1#qunit-header
    %h2#qunit-banner
    %h2#qunit-userAgent
    %ol#qunit-tests
    #qunit-fixture
      = yield
      .modal_fixtures
        .default
          #modal
          #modal_layer
        .custom_name
          #other_modal
          #modal_layer
          